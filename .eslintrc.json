{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "airbnb",
      "plugin:prettier/recommended"
  ],
  "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "rules": {
      "prettier/prettier":[
          "error",
          {
              "singleQuote": true,
              "semi": true,
              "trailingComma": "all",
              "arrowParens": "avoid"
          }
        ]
  }
}
